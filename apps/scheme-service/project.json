{
  "name": "scheme-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/scheme-service/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/scheme-service",
        "main": "apps/scheme-service/src/main.ts",
        "tsConfig": "apps/scheme-service/tsconfig.app.json",
        "assets": ["apps/scheme-service/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/scheme-service/src/environments/environment.ts",
              "with": "apps/scheme-service/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "scheme-service:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/scheme-service/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/scheme-service"],
      "options": {
        "jestConfig": "apps/scheme-service/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:scheme-service", "type:app"]
}